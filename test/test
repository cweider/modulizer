#!/bin/bash

RESULT=`../modulize --root-path root --library-path lib --import-libraries --import-root --include-kernel -- ext`
TEST=`cat test.js`
echo "$RESULT
$TEST" > code.js
node -e "$RESULT
$TEST" > actual.out
if ! diff expected.out actual.out
then
	echo "FAIL: Output does not match expected."
else
	echo "PASS"
fi
